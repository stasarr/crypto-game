<%- include('../partials/header') %>

<div class="container mt-5">
  <h2>Kullanıcılar</h2>
  <ul class="list-group">
    <% users.forEach(user => { %>
      <li class="list-group-item d-flex justify-content-between align-items-center bg-dark text-lgray">
        <%= user.username %>
        <span class="badge bg-success rounded-pill">Puan: <%= user.score %></span>
      </li>
    <% }) %>
  </ul>
</div>

<%- include('../partials/footer') %>
